{
    "name": "stringphp/router",
    "description": "Router for amphp/http-server",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StringPhp\\Router\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "authors": [
        {
            "name": "Robert Snedeker",
            "email": "rsnedeker20@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "amphp/amp": "^3.0",
        "revolt/event-loop": "^1.0",
        "amphp/http-server": "^3.3",
        "monolog/monolog": "^3.6",
        "amphp/log": "^2.0",
        "stringphp/utils": "dev-main",
        "httpsoft/http-message": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16",
        "pestphp/pest": "^2.34"
    },
    "scripts": {
        "fix": "php-cs-fixer fix --using-cache=no",
        "fix:dry": "php-cs-fixer fix --using-cache=no --diff --dry-run"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}
